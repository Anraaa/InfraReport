{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@ardent-data/vercel-php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|assets|favicon.ico|robots.txt|build/.*|storage/.*|vendor/.*)",
            "dest": "/public/$0"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/public/index.php"
        }
    ]
}